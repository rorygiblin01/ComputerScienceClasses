		btnCalc.addActionListener(new ActionListener()
		{
			public void actionPerformed(ActionEvent e)
			{
				try 
				{
					System.out.println("test");
					String hw00Value = textFieldHW00.getText();
					String hw01Value = textFieldHW01.getText();
					String hw02Value = textFieldHW02.getText();
					String hw03Value = textFieldHW03.getText();
					String hw04Value = textFieldHW04.getText();
					String lab00Value = textFieldLab00.getText();
					String lab01Value = textFieldLab01.getText();
					String lab02Value = textFieldLab02.getText();
					String lab03Value = textFieldLab03.getText();
					String lab04Value = textFieldLab04.getText();
					String lab05Value = textFieldLab05.getText();
					String lab06Value = textFieldLab06.getText();
					String lab07Value = textFieldLab07.getText();
					String lab08Value = textFieldLab08.getText();
					String lab09Value = textFieldLab09.getText();
					String lab10Value = textFieldLab10.getText();
					String lab11Value = textFieldLab11.getText();
					String lab12Value = textFieldLab12.getText();
					String extraCreditValue = textFieldExtraCredit.getText();
					int hw00ValueInt = Integer.valueOf(hw00Value);
					int hw01ValueInt = Integer.valueOf(hw01Value);
					int hw02ValueInt = Integer.valueOf(hw02Value);
					int hw03ValueInt = Integer.valueOf(hw03Value);
					int hw04ValueInt = Integer.valueOf(hw04Value);
					int lab00ValueInt = Integer.valueOf(lab00Value);
					int lab01ValueInt = Integer.valueOf(lab01Value);
					int lab02ValueInt = Integer.valueOf(lab02Value);
					int lab03ValueInt = Integer.valueOf(lab03Value);
					int lab04ValueInt = Integer.valueOf(lab04Value);
					int lab05ValueInt = Integer.valueOf(lab05Value);
					int lab06ValueInt = Integer.valueOf(lab06Value);
					int lab07ValueInt = Integer.valueOf(lab07Value);
					int lab08ValueInt = Integer.valueOf(lab08Value);
					int lab09ValueInt = Integer.valueOf(lab09Value);
					int lab10ValueInt = Integer.valueOf(lab10Value);
					int lab11ValueInt = Integer.valueOf(lab11Value);
					int lab12ValueInt = Integer.valueOf(lab12Value);
					int extraCreditValueInt = Integer.valueOf(extraCreditValue);
					double theSum = 0;
					double theAverage = 0;
					theSum = ((hw00ValueInt + hw01ValueInt + hw02ValueInt + hw03ValueInt + hw04ValueInt + lab00ValueInt
						+ lab01ValueInt + lab02ValueInt + lab03ValueInt + lab04ValueInt + lab05ValueInt + lab06ValueInt 
						+ lab07ValueInt + lab08ValueInt + lab09ValueInt + lab10ValueInt + lab11ValueInt + lab12ValueInt));
					theAverage = (100*extraCreditValueInt) + theSum;
					theAverage = theSum/18;
					lblAnswer.setText(String.format("%.2f%%",theAverage));
				}
				catch(NumberFormatException error)
				{
					lblAnswer.setText("Number Format Exception: " + error.getMessage());
				}
			}
		});

		}
